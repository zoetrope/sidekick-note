[client]
default-character-set = utf8

[mysqld]
basedir = /opt/mysql/server-5.6
datadir = /var/opt/mysql

skip-character-set-client-handshake
character-set-server = utf8

ft_min_word_len=1
innodb_ft_min_token_size=1
innodb_ft_max_token_size=16

[mysqldump]
default-character-set = utf8

[mysql]
default-character-set = utf8

