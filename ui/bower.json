{
  "name": "sidekick-note-ui",
  "version": "0.0.1",
  "homepage": "https://github.com/zoetrope/sidekick-note",
  "authors": [
    "zoetrope"
  ],
  "description": "UI for sidekick-note",
  "main": "index.js",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.1.0",
    "bootstrap": "~3.1.0",
    "angular": "~1.2.12",
    "angular-resource": "~1.2.12",
    "angular-route": "~1.2.12",
    "angular-ui": "~0.4.0",
    "angular-ui-utils": "~0.1.1",
    "angular-ui-select2": "~0.0.5",
    "angular-ui-bootstrap-bower": "0.10.0",
    "angular-ui-date": "~0.0.3",
    "jquery-ui": "~1.10.3",
    "select2": "3.4.5",
    "marked": "~0.3.1",
    "highlightjs": "~8.0.0",
    "rxjs": "~2.2.13",
    "reactiveproperty-angular": "~0.2.0",
    "momentjs": "~2.5.1"
  },
  "devDependencies": {
    "jasmine": "~1.3.1",
    "angular-mocks": "~1.2.1"
  },
  "exportsOverride": {
    "angular": {
      "public/scripts/libs": "**/angular.js"
    },
    "angular-resource": {
      "public/scripts/libs": "**/angular-resource.js"
    },
    "angular-route": {
      "public/scripts/libs": "angular-route.js"
    },
    "angular-ui": {
      "public/scripts/libs": "build/angular-ui.js",
      "public/stylesheets": "build/angular-ui.css"
    },
    "angular-ui-select2": {
      "public/scripts/libs": "src/select2.js"
    },
    "angular-ui-date": {
      "public/scripts/libs": "src/date.js"
    },
    "angular-ui-utils": {
      "public/scripts/libs": "ui-utils.js"
    },
    "select2": {
      "public/scripts/libs": "select2.js",
      "public/stylesheets": [
        "select2.css",
        "select2-spinner.gif",
        "select2.png",
        "select2x2.png"
      ]
    },
    "angular-ui-bootstrap-bower": {
      "public/scripts/libs": "ui-bootstrap-tpls.js"
    },
    "bootstrap": {
      "public/scripts/libs": "dist/js/bootstrap.js",
      "public/stylesheets": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap-theme.css"
      ],
      "public/fonts": "dist/fonts/*.*"
    },
    "highlightjs": {
      "public/scripts/libs": "highlight.pack.js",
      "public/stylesheets": "styles/default.css"
    },
    "jquery": {
      "public/scripts/libs": "**/jquery.js"
    },
    "jquery-ui": {
      "public/scripts/libs": [
        "ui/jquery.ui.core.js",
        "ui/jquery.ui.datepicker.js"
      ],
      "public/stylesheets": [
        "themes/smoothness/jquery-ui.css"
      ],
      "public/images": [
        "themes/smoothness/images/*"
      ]
    },
    "marked": {
      "public/scripts/libs": "lib/marked.js"
    },
    "momentjs": {
      "public/scripts/libs": [
          "moment.js",
          "lang/ja.js"
      ]
    },
    "rxjs": {
      "public/scripts/libs": [
        "rx.js",
        "rx.async.js"
      ]
    },
    "reactiveproperty-angular": {
      "public/scripts/libs": [
        "reactiveproperty-angular.js"
      ]
    },
    "angular-mocks": {
      "ui/src/test/libs": "**/*.js"
    },
    "jasmine": {
      "ui/src/test/libs": "lib/jasmine-core/jasmine*.js",
      "ui/src/test/stylesheets": "lib/jasmine-core/*.css"
    }
  }
}
