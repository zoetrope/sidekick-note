{
  "name": "sidekick-note-ui",
  "version": "0.0.1",
  "homepage": "https://github.com/zoetrope/sidekick-note",
  "authors": [
    "zoetrope"
  ],
  "description": "UI for sidekick-note",
  "main": "index.js",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.0.3",
    "bootstrap": "~3.0.0",
    "angular": "~1.0.8",
    "angular-resource": "~1.0.8",
    "angular-ui": "~0.4.0",
    "marked": "~0.2.9",
    "highlightjs": "~7.3.0",
    "angular-ui-utils": "~0.0.4",
    "angular-ui-select2": "~0.0.2",
    "angular-ui-bootstrap": "~0.6.0"
  },
  "devDependencies": {
    "jasmine": "~1.3.1",
    "angular-mocks": "~1.0.8"
  },
  "exportsOverride": {
    "angular": {
      "public/scripts/libs": "**/angular.js"
    },
    "angular-resource": {
      "public/scripts/libs": "**/angular-resource.js"
    },
    "angular-ui": {
      "public/scripts/libs": "build/angular-ui.js",
      "public/stylesheets": "build/angular-ui.css"
    },
    "angular-ui-select2": {
      "public/scripts/libs": "src/select2.js"
    },
    "angular-ui-utils": {
      "public/scripts/libs": "modules/keypress/keypress.js"
    },
    "angular-ui-bootstrap": {
      "public/scripts/libs": "dist/ui-bootstrap-tpls-0.6.0.js"
    },
    "select2": {
      "public/scripts/libs": "select2.js",
      "public/stylesheets": [
        "select2.css",
        "select2-spinner.gif",
        "select2.png",
        "select2x2.png"
      ]
    },
    "bootstrap": {
      "public/scripts/libs": "dist/js/bootstrap.js",
      "public/stylesheets": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap-theme.css"
      ],
      "public/fonts": "dist/fonts/*.*"
    },
    "highlightjs": {
      "public/scripts/libs": "highlight.pack.js",
      "public/stylesheets": "styles/default.css"
    },
    "jquery": {
      "public/scripts/libs": "**/jquery.js"
    },
    "marked": {
      "public/scripts/libs": "lib/marked.js"
    },
    "angular-mocks": {
      "ui/src/test/libs": "**/*.js"
    },
    "jasmine": {
      "ui/src/test/libs": "lib/jasmine-core/jasmine*.js",
      "ui/src/test/stylesheets": "lib/jasmine-core/*.css"
    }
  }
}
